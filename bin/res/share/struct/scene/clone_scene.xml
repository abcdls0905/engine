<!--场景副本属性-->
<object>
    <!-- 基本属性 -->
    <properties>
        <property name="CloneSceneID" type="dword" public="false" private="false" save="false" desc="副本场景编号" />
        <property name="CloneDifficulty" type="dword" public="false" private="false" save="false" desc="副本难度(0-初级 1-中级 2-高级)" />
        <property name="PassStageTime" type="int64" public="false" private="false" save="false" desc="通关时间" />
        <property name="ScenePlayer" type="object" public="false" private="false" save="false" desc="副本中的玩家对象（用于本地副本）" />
		
		<property name="NpcBeKilledCount" type="dword" public="false" private="false" save="false" desc="副本场景NPC被击杀总数" />
    </properties>
    <records>
		<record name="clone_record_team_list" cols="5" maxrows="10" save="false" public="false" private="false" desc="组队副本队友信息">
            <column type="string" desc="组队成员uid，第一个是队长" />
            <column type="dword" desc="是否生存" />
            <column type="dword" desc="RoleIndex" />
            <column type="dword" desc="Level" />
            <column type="dword" desc="FightCap" />
        </record>
		<record name="clone_pull_player_rec" cols="1" maxrows="128" save="false" public="false" private="false" desc="尝试拉入副本的玩家名字表">
            <column type="widestr" desc="玩家名字" />
        </record>
    </records>
</object>
