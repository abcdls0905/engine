<?xml version="1.0" encoding="gb2312"?>
<!-- edited with XMLSpy v2005 rel. 3 U (http://www.altova.com) by  () -->
<!--Equip_Item的属性表，继承自Nim_Item，所有可装备的物品的基类-->
<!--SlotType 孔位类型 1,2,3,4,5分别代表金木水火土-->
<object>
	<!-- 基本属性 -->
	<properties>
		<property name="RuneWordsPackage" type="dword" public="false" private="true" save="true" desc="符文之语属性包"/>
		<property name="BasePropertyPackage" type="string" public="false" private="true" save="true" desc="符文属性包"/>
		<property name="SlotType" type="byte" public="false" private="true" save="true" desc="可镶嵌的孔位类型"/>
		<property name="SuitRoleId" type="byte" public="false" private="true" save="true" desc="可以使用该符文的职业"/>
		<property name="CompondGrade" type="dword" public="false" private="true" save="true" desc="合成等级"/>
		<property name="NpcLevel" type="dword" public="false" private="true" save="true" desc="使用该符文的英雄等级"/>
		<property name="RoleFit" type="string" public="false" private="true" save="true" desc="可以使用该核心符文的英雄列表"/>
		<property name="PropModifyPack" type="int" public="false" private="true" save="true" desc="属性修正包"/>
		<property name="TattooAdvExp" type="int" public="false" private="true" save="true" realtime="true" desc="符文经验值(充能值)"/>
		<property name="TattooAdvLvl" type="int" public="false" private="true" save="true" realtime="true" desc="符文进阶等级"/>
		<property name="TattooPropLvl" type="int" public="false" private="true" save="true" realtime="true" desc="符文进阶属性等级"/>
	</properties>
	<!--策划要求装备的修正表必须保存不得重刷-->
	<records>
		<record name="SymTattooUpgradeRec" cols="8" maxrows="16" save="true" public="false" private="true" desc="符文升级解锁属性表">
			<column type="string" desc="属性名"/>
			<column type="DWORD" desc="属性值"/>
			<column type="DWORD" desc="升级增加值"/>
			<column type="DWORD" desc="解锁属性类型"/>
			<column type="DWORD" desc="解锁等级限制"/>
			<column type="byte" desc="是否为激活状态1为激活0为不激活即不能起到作用"/>
			<column type="string" desc="说明文字ID"/>
			<column type="string" desc="说明文字ID"/>
		</record>
		<record name="SymTattooRuneWordsRec" cols="3" maxrows="16" save="true" public="false" private="true" desc="符文之语属性表">
			<column type="string" desc="属性ID"/>
			<column type="string" desc="属性名"/>
			<column type="DWORD" desc="属性值"/>
		</record>
		<record name="PropModifySymbolTattooRec" cols="3" maxrows="300" save="false" public="true" private="true" desc="符文属性刷新表">
			<column type="string" desc="属性名"/>
			<column type="float" desc="属性值"/>
			<column type="string" desc="来源"/>
		</record>
	</records>
</object>
