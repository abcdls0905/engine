<!--Equip_Item的属性表，继承自Nim_Item，所有可装备的物品的基类-->
<object>
	<!-- 基本属性 -->
	<properties>
	    <property name="FashionType" type="string" public="false" private="true" save="false" desc="装备栏位类型, 见EquipDefine.ini文件中的定义"/>
		<property name="FightingCapacity" type="int" public="false" private="true" save="true" desc="装备战斗力"/>
		<property name="ColorLabel" type="int" public="false" private="true" save="true" desc="颜色标签"/>
        <property name="AMExpend" type="string" public="false" private="true" save="true" desc="附魔消耗ID"/>
        <property name="AMPropRandom" type="string" public="false" private="true" save="true" desc="附魔随机属性"/>
        <property name="AdheringMagicTime" type="int" public="false" private="true" save="true" realtime="true" desc="附魔次数"/>
		<property name="FashionSuitId" type="int" public="false" private="true" save="true" desc="时装的套装id"/>
		<property name="jcsx" type="int" public="false" private="false" save="false" desc="基础属性"/>
	</properties>
	
	<!---->
	<records>
        <record name="FashionAdheringMagicRec" cols="6" maxrows="300" save="true" public="false" private="true" desc="时装附魔属性表">
            <column type="string" desc="属性名"/>                                                                                    
            <column type="DWORD" desc="属性值"/>                                                                                     
            <column type="DWORD" desc="是否锁定"/>                                                                                   
            <column type="string" desc="说明文字ID"/>
            <column type="string" desc="说明文字ID"/>
            <column type="DWORD" desc="颜色"/>                                                                                     
       </record>
	</records>
</object>
