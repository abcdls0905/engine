		<record name="enemy_rec" cols="6" maxrows="100" save="true" public="false" private="true" desc="仇人">
			<column type="string"   desc="仇人UID" />
			<column type="dword"    desc="仇人职业" />
            <column type="widestr"  desc="仇人名称" />
