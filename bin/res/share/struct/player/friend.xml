    <records>
        <record name="friend_rec" 	cols="6" maxrows="300" save="true" public="false" private="true" desc="好友列表">
            <column type="string"   desc="对方UID" />
			<column type="dword"    desc="好友职业" />
            <column type="widestr"  desc="好友名称" />
            <column type="dword"    desc="好友是否在线" />
            <column type="dword"    desc="好友等级" />
            <column type="dword"    desc="好友战斗力" />
        </record>
		<record name="blacklist_rec" cols="6" maxrows="100" save="true" public="false" private="true" desc="黑名单">
			<column type="string" 	desc="黑名单UID" />
			<column type="dword"    desc="黑名单职业" />
            <column type="widestr"  desc="黑名单名称" />
            <column type="dword"    desc="黑名单是否在线" />
            <column type="dword"    desc="黑名单等级" />
