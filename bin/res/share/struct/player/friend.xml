            <column type="widestr"  desc="好友名称" />
            <column type="dword"    desc="好友等级" />
            <column type="dword"    desc="好友战斗力" />
            <column type="dword"    desc="推荐头像索引" />
            <column type="dword"    desc="门派索引" />
        </record>
    </records>
    <includes />
</object>
