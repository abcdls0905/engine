<?xml version="1.0" encoding="utf-8" ?>
<object>
    <internals />
    <properties>
        <property name="GiveGiftTime" 		type="int64" public="false" private="false" save="true" realtime="" desc="记录赠送礼物的时间" />
        <property name="GiveGiftCount"      type="dword" public="false" private="false" save="true" realtime="" desc="记录赠送礼物的次数" />
        <property name="GetGiftTime"        type="int64" public="false" private="false" save="true" realtime="" desc="记录收取礼物的时间" />
        <property name="GetGiftCount"       type="dword" public="false" private="false" save="true" realtime="" desc="记录收取礼物的次数" />
        <property name="GetGiftCountMax"    type="dword" public="false" private="false" save="true" realtime="" desc="记录每日收取礼物的最大次数" />
        <property name="GiveGiftCountMax"   type="dword" public="false" private="false" save="true" realtime="" desc="记录每日收取礼物的最大次数" />
