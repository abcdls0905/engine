<object>
    <!-- 基本属性 -->
    <properties>
        <property name="PayAlready" type="dword" public="false" private="true" save="true" desc="已经充过值" />
        <property name="VipLevel" type="dword" public="false" private="true" save="true" desc="VIP等级" />
        <property name="TotalPay" type="dword" public="false" private="true" save="true" desc="累计充值" />
        <property name="ItemShopResetDate" type="string" public="false" private="false" save="true" desc="道具商店次数重置日期" />
		<property name="HaveCardType" type="dword" public="false" private="true" save="true" desc="购买的卡类型" />
        <property name="CardEndDate" type="double" public="false" private="true" save="true" desc="卡截止日期" />
    </properties>
    <!-- 表属性 -->
    <records>
        <record name="item_mall_bought_rec" cols="4" maxrows="100" save="true" public="false" private="true" desc="玩家购买道具记录表">
          <column type="dword" desc="橱窗ID" />
		  <column type="string" desc="道具ID" />
          <column type="dword" desc="道具数量" />
		  <column type="int64" desc="限时时间内第一次购买时间" />
        </record>
        <record name="plus_item_shop_bought_rec" cols="5" maxrows="100" save="true" public="false" private="true" desc="玩家购买运营礼包记录表">
          <column type="dword" desc="礼包ID" />
		  <column type="string" desc="版本号" />
          <column type="dword" desc="已购买数量" />
          <column type="dword" desc="限购数量" />
		  <column type="int64" desc="限时时间内第一次购买时间" />
        </record>
        <record name="buy_vip_item_bought_rec" cols="2" maxrows="30" save="true" public="false" private="true" desc="玩家购买vip福利记录">
          <column type="dword" desc="编号" />
          <column type="dword" desc="是否购买 0：没有 1：买了" />
        </record>
        <record name="first_pay_bought_rec" cols="2" maxrows="30" save="true" public="false" private="true" desc="玩家购买钻石分类别记录 ">
          <column type="dword" desc="编号" />
          <column type="dword" desc="是否购买该类 0：没有 1：买了" />
        </record>
        <record name="pay_gift_rec" cols="3" maxrows="20" save="true" public="false" private="true" desc="充值返利表">
            <column type="dword" desc="充值数量" />
            <column type="string" desc="是否可以领取(解锁日期)" />
            <column type="string" desc="领取的日期" />
        </record>
    </records>
</object>
