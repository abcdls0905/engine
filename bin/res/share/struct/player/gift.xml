<?xml version="1.0" encoding="gb2312" ?>
<object>
    <properties>
        <property name="ServerCurrentDayTime" type="string" public="false" private="true" save="true" realtime="" desc="服务器当前日期" />
        <property name="AlreadyRepairTimes" type="dword" public="false" private="true" save="true" realtime="" desc="玩家已补签次数" />
        <property name="VipGiftGet" type="string" public="false" private="true" save="true" realtime="" desc="玩家是否已领取VIP礼包" />
        <property name="IsSign" type="string" public="false" private="true" save="true" realtime="true" desc="玩家是否签到" />
        <property name="MonthCardGiftGet" type="byte" public="false" private="true" save="true" realtime="true" desc="玩家是否已领取月卡、季卡、年卡的每日额外奖励" />
        <property name="FuLiCardGiftGet" type="byte" public="false" private="true" save="true" realtime="true" desc="玩家是否已领取福利卡的奖励" />
		<property name="PhoneBindState" type="int" public="false" private="true" save="true" realtime="true" desc="绑定手机状态（0未绑定；1已绑定）" />
		<property name="PhoneBindGiftState" type="int" public="false" private="true" save="true" realtime="true" desc="绑定手机状态礼品领取状态（0未领取；1已领取）" />
		<property name="SignInBeginTime" type="int64" public="false" private="false" save="true" realtime="" desc="本轮签到开始时间" />
		<property name="SignInLastTime" type="int64" public="false" private="true" save="true" realtime="" desc="最后一次签到的时间" />
		<property name="SignInCount" type="int" public="false" private="true" save="true" realtime="" desc="本轮签到次数" />
		<property name="RepairSignInCount" type="int" public="false" private="true" save="true" realtime="" desc="本轮补签次数" />
    </properties>
    <records>
        <record name="sign_in_rec" cols="2" maxrows="31" save="true" public="false" private="true" desc="签到礼包">
            <column type="byte" desc="签到状态" />
            <column type="byte" desc="条件奖励领取状态" />
        </record>
		<record name="everyday_gift_rec" cols="2" maxrows="100" save="true" public="false" private="true" desc="每日登陆礼包">
            <column type="dword" desc="解锁天数" />
            <column type="string" desc="领取的日期" />
        </record>
    </records>
</object>
