<?xml version="1.0" encoding="utf-8" ?>
<object>
    <properties>
	    <property name="IsActivatedW1G" type="int" public="false" private="true" save="true" realtime="" desc="是否激活了W1手机返利" />   
		<property name="IsActivatedW1P" type="int" public="false" private="true" save="true" realtime="" desc="是否激活了W1手机特权" />
		<property name="IsMCardAccount" type="int" public="false" private="true" save="true" realtime="" desc="是否是免卡用户" />
	</properties>
    <records>
        <record name="w1_gain_rec" cols="2" maxrows="200" save="true" public="false" private="true" desc="w1手机返利领取记录">
            <column type="dword" desc="可领取需要达到的等级" />
            <column type="dword" desc="是否已经领取"/>
        </record>
		<record name="w1_privilege_rec" cols="5" maxrows="200" save="true" public="false" private="true" desc="w1手机特权领取记录">
			<column type="dword" desc="奖励ID" />
			<column type="dword" desc="奖励类型（一次性奖励、循环奖励）" />
			<column type="dword" desc="领奖的时间间隔(单位：天)" />
			<column type="int64" desc="最后一次领取的时间"/>
			<column type="dword" desc="领取奖励状态(可领取、不可领取）"/>
		</record>
		<record name="m_card_rec" cols="4" maxrows="200" save="true" public="false" private="true" desc="免卡特权领取记录">
			<column type="dword" desc="奖励ID" />
			<column type="dword" desc="领奖的时间间隔(单位：天)" />
			<column type="int64" desc="最后一次领取的时间"/>
			<column type="dword" desc="领取奖励状态(可领取、不可领取）"/>
		</record>
	</records>
</object>