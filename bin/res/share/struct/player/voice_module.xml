        <!-- 表属性 -->
        <records>
        <!-- 已经加入的语音频道 -->
        <record name="RecPlayerChannel" cols="5" maxrows="100" save="false" public="false" private="true" desc="已加入的语音频道">
			<column type="dword"  desc="房间类型" />
            <column type="string" desc="房间名" />
            <column type="string"  desc="房间key" />
			<column type="string"  desc="房间扩展参数" />
			<column type="dword"  desc="加入房间状态" />
