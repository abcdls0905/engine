<?xml version="1.0" encoding="utf-8"?>
<object>
	<properties>
		<property name="FestivalLastLoginDate" type="string" public="false" private="true" save="true" realtime="" desc="最后一次登陆的时间"/>
		<property name="PetActivityCount" type="int" public="false" private="true" save="true" realtime="" desc="限时武神抽取次数"/>
		<property name="LimitActivityCount" type="int" public="false" private="true" save="true" realtime="" desc="限时英雄秘境扫荡或进入次数"/>
		<property name="PetActivityFreeOpenTime" type="int64" public="false" private="true" save="true" realtime="" desc="限时武神最后一次免费开启的时间"/>
		<property name="SevenDaysBeginTime" type="int64" public="false" private="true" save="true" realtime="" desc="七天活动上次登录领取时间"/>
		<property name="FestivalOnlineBeginTime" type="int64" public="false" private="true" save="true" realtime="" desc="累计在线时间活动"/>
		<property name="SevenDaysBuyDiamondCount" type="int" public="false" private="true" save="true" realtime="" desc="七天活动累计充钻数值"/>
		<property name="SevenDaysSignTimes" type="int" public="false" private="true" save="true" realtime="" desc="七天活动登录次数"/>
		<property name="AppStoreComment" type="byte" public="false" private="true" save="true" realtime="" desc="appstore评论提示"/>
		<property name="SpringFreeTimes" type="byte" public="false" private="true" save="true" realtime="" desc="春节活动每日免费次数"/>
		<property name="TCResetDate" type="int64" public="false" private="false" save="true" realtime="" desc="翻牌活动重置次数的日期"/>
		<property name="TCFreeResetCount" type="int" public="false" private="true" save="true" realtime="" desc="当天免费重置次数"/>
		<property name="TCCardState" type="int" public="false" private="true" save="true" realtime="" desc="翻牌活动牌的状态"/>
		<property name="TurnCardOrder" type="string" public="false" private="false" save="true" realtime="" desc="翻牌产出道具类型的次序"/>
		<property name="TCOutputMultiple" type="int" public="false" private="false" save="true" realtime="" desc="下次翻牌产出倍数"/>
		<property name="OneDayTotalPay" type="int" public="false" private="true" save="true" realtime="" desc="单日累冲数"/>
		<property name="OneDayTotalPayRefresh" type="int" public="false" private="true" save="true" realtime="" desc="重置充值时间"/>
		<property name="PetTimes" type="int" public="false" private="false" save="true" realtime="" desc="限时英雄抽取次数"/>
		<property name="TreasureTimes" type="int" public="false" private="false" save="true" realtime="" desc="宝藏抽取次数"/>
		<property name="AnswerState" type="byte" public="false" private="true" save="true" realtime="" desc="问答活动状态"/>
		<property name="RaffleAward" type="string" public="false" private="true" save="false" realtime="true" desc="抽奖副本掉落"/>
		<property name="PayGoldRaffleTimes" type="dword" public="false" private="true" save="true" realtime="" desc="付费金币抽奖副本次数"/>
		<property name="PayDiamondRaffleTimes" type="dword" public="false" private="true" save="true" realtime="" desc="付费钻石抽奖副本次数"/>
		<property name="PayLimitHeroTimes" type="dword" public="false" private="true" save="true" realtime="" desc="付费限时英雄副本次数"/>
		<property name="HideNewServerCharge" type="int" public="false" private="false" save="true" realtime="" desc="隐藏新服充值活动，老版本角色设为1"/>
		<property name="CollectBuyCount" type="int" public="false" private="true" save="true" realtime="" desc="集字兑换次数"/>
		<property name="CollectBuyDay" type="double" public="false" private="false" save="true" realtime="" desc="集字兑换日期"/>
		<property name="CollectRefreshTime" type="int64" public="false" private="true" save="true" realtime="" desc="集字刷新时间"/>
		<property name="AnniversaryState" type="byte" public="false" private="true" save="false" realtime="" desc="周年庆活动开启状态" />
		<property name="ActStateForClone" type="byte" public="false" private="true" save="fakse" realtime="" desc="活动状态(副本中使用)"/>
	</properties>
	<records>
		<record name="festival_sub_rec" cols="9" maxrows="500" save="true" public="false" private="true" desc="节日活动子包表格">
			<column type="dword" desc="所属活动ID"/>
			<column type="dword" desc="活动子包ID"/>
			<column type="dword" desc="解锁条件数值"/>
			<column type="dword" desc="是否已经领取了奖励"/>
			<column type="dword" desc="活动版本号"/>
			<column type="dword" desc="活动累计登陆天数"/>
			<column type="byte" desc="是否加入公服活动名单"/>
			<column type="dword" desc="刷新登陆时间"/>
			<column type="dword" desc="刷新消费记录时间"/>
		</record>
		<record name="word_collect_record" cols="1" maxrows="5" save="true" public="false" private="true" desc="集字活动表">
		    <column type="string"  desc="礼包编号" />
        </record>
		<record name="festival_new_rec" cols="3" maxrows="500" save="true" public="false" private="true" desc="节日活动提示表格">
			<column type="byte" desc="所属活动ID"/>
			<column type="byte" desc="是否是首次开启活动"/>
			<column type="byte" desc="活动版本号"/>
		</record>
		<record name="treasure_shop_rec" cols="5" maxrows="100" save="true" public="false" private="true" desc="宝藏积分商店表">
			<column type="dword" desc="物品编号"/>
			<column type="string" desc="物品ID"/>
			<column type="dword" desc="物品剩余个数"/>
			<column type="dword" desc="消耗"/>
			<column type="dword" desc="是否刷新"/>
		</record>
		<record name="awake_shop_rec" cols="5" maxrows="100" save="true" public="false" private="true" desc="附魔积分商店表">
			<column type="dword" desc="物品编号"/>
			<column type="string" desc="物品ID"/>
			<column type="dword" desc="物品剩余个数"/>
			<column type="dword" desc="消耗"/>
			<column type="dword" desc="是否刷新"/>
		</record>
		<record name="seven_days_rec" cols="4" maxrows="50" save="true" public="false" private="true" desc="七天活动表格">
			<column type="dword" desc="天数"/>
			<column type="dword" desc="累计充值领奖状态"/>
			<column type="dword" desc="每日登陆领奖状态"/>
			<column type="dword" desc="半价礼包购买状态"/>
		</record>
		<record name="new_server_charge_rec" cols="4" maxrows="50" save="true" public="false" private="true" desc="新服充值活动表">
			<column type="byte" desc="天数"/>
			<column type="double" desc="登录日期"/>
			<column type="dword" desc="当日充值金额"/>
			<column type="string" desc="奖励领取状态"/>
		</record>
		<record name="turn_card_rec" cols="5" maxrows="50" save="true" public="false" private="true" desc="翻牌表格">
			<column type="dword" desc="牌的位置"/>
			<column type="dword" desc="牌的类型"/>
			<column type="string" desc="物品ID"/>
			<column type="dword" desc="物品数量"/>
			<column type="dword" desc="是否翻开"/>
		</record>
		<record name="good_music_self_vote_rec" cols="2" maxrows="50" save="true" public="false" private="true" desc="好声音活动打赏记录">
			<column type="string" desc="榜单歌曲id"/>
			<column type="dword" desc="已打赏次数"/>
		</record>
	</records>
</object>
