<!--当前玩家拥有的副本ID-->
<object>
    <!-- 基本属性 -->
    <properties>
        <property name="CloneResetDate" type="double" public="false" private="false" save="true" desc="副本重置日期"/>
    </properties>
    <records>
        <record name="clone_get_record" cols="4" maxrows="300" save="true" public="false" private="false" desc="副本获得信息表">
            <column type="dword" desc="物品类型" />
            <column type="string" desc="物品名称" />
            <column type="dword" desc="物品数量" />
            <column type="byte" desc="绑定" />
        </record>
		<record name="drop_items_rec" cols="4" maxrows="100" save="false" public="false" private="false" need_defined="room" desc="掉落物品记录（只房间记录）">
			<column type="dword" desc="类型"/>
			<column type="string" desc="物品config"/>
			<column type="dword" desc="物品数量"/>
			<column type="byte" desc="绑定"/>
		</record>
		<record name="pickup_items_rec" cols="4" maxrows="100" save="false" public="false" private="false" desc="拾取物品记录">
			<column type="dword" desc="类型"/>
			<column type="string" desc="物品config"/>
			<column type="dword" desc="物品数量"/>
			<column type="byte" desc="绑定"/>
		</record>
		<!-- 新副本模块使用 宛俊 -->
		<record name="owner_clone_info_rec" cols="6" maxrows="600" save="true" public="false" private="true" desc="玩家拥有的副本信息表">
            <column type="dword" desc="副本原型id" />
			<column type="dword" desc="剩余进入次数" />
			<column type="dword" desc="是否开启" />
			<column type="dword" desc="副本当前进度" />
			<column type="dword" desc="副本类型，服务器副本有效" />
            <column type="string" desc="副本唯一UID，服务器副本有效" />
        </record>
		<record name="clone_pass_rec" cols="3" maxrows="300" save="true" public="false" private="true" desc="已经通关的副本表">
            <column type="dword" desc="副本原型id" />
            <column type="dword" desc="星级" />
            <column type="dword" desc="通关时间" />
        </record>
		<record name="clone_reset_rec" cols="2" maxrows="300" save="true" public="false" private="true" desc="挑战次数重置次数">
            <column type="dword" desc="副本原型id" />
            <column type="dword" desc="挑战次数被重置次数" />
        </record>
		<record name="clone_follower_rec" cols="5" maxrows="32" save="false" public="false" private="false" desc="跟随进入副本的战友表">
            <column type="widestr" desc="战友名字" />
			<column type="dword" desc="战友职业" />
			<column type="dword" desc="战友职业等级" />
			<column type="dword" desc="战友职业分支" />
			<column type="dword" desc="战友状态" />
        </record>
		<record name="clone_match_rec" cols="3" maxrows="100" save="false" public="false" private="false" desc="正在匹配的副本表">
            <column type="dword" desc="副本原型id" />
			<column type="widestr" desc="主玩家名字" />
			<column type="int64" desc="报名时间" />
        </record>
    </records>
</object>
