<?xml version="1.0" encoding="utf-8" ?>
<object>
    <internals />
    <properties>
        <property name="GuildName" type="widestr" public="true" private="true" save="true" realtime="" desc="公会名称" />
		<property name="GuildPosition" type="int" public="true" private="true" save="true" realtime="" desc="在公会中的职务" />
		<property name="QuitGuildDate" type="int64" public="false" private="false" save="true" realtime="" desc="退出公会日期" />
		<property name="JoinGuildDate" type="int64" public="false" private="false" save="true" realtime="" desc="加入公会日期" />
		<property name="GuildDevote" type="int" public="false" private="false" save="true" realtime="" desc="玩家个人公会总贡献度" />
		<property name="GuildLeftDevote" type="int" public="false" private="true" save="true" realtime="" desc="玩家个人公会剩余贡献度" />
		<property name="GuildListBeginRow" type="int" public="false" private="false" save="false" realtime="" desc="获取公会列表的起始行" />
		<property name="GuildLastBuyDate" type="int64" public="false" private="false" save="true" realtime="" desc="最后一次兑换商品的时间" />
