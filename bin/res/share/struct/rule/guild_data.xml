			<column type="dword" desc="每日兑换个数限制" />
		</record>
		<record name="GuildSSBossAwardTimeRec" cols="2" maxrows="16" save="true" public="false" private="false" desc="公会野外boss可领奖时间">
			<column type="int" desc="ssboss_id" />
			<column type="int64" desc="可领奖时间" />
		</record>
	</records>
	<includes />
</object>
