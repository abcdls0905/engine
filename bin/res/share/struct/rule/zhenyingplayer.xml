<?xml version="1.0" encoding="gb2312"?>
<!--时装系统相关的属性-->

<object>
  <!-- 基本属性 -->
  <properties>
    <property name="ZhenYingChangeCount" type="dword" public="false" private="true" save="true" desc="换阵营次数"/>
    <property name="ZhenYingShopRefreshDate" type="string" public="false" private="false" save="true" realtime="" desc="阵营商店刷新日期(YYYY_MM_DD)"/>
  </properties>
  <records>
    <record name="zhenying_shop_rec" cols="5" maxrows="100" save="true" public="false" private="true" desc="阵营商店表">
      <column type="dword" desc="物品编号"/>
      <column type="string" desc="物品ID"/>
      <column type="dword" desc="物品剩余个数"/>
      <column type="dword" desc="消耗"/>
      <column type="dword" desc="是否刷新"/>
    </record>
  </records>
</object>
