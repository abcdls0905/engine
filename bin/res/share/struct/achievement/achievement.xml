<!--游戏对象属性定义模版文件-->
<object>
	<properties>
		<property name="AchievePoint" type="int" public="false" private="true" save="true" desc="成就点"/>
		<property name="PlayKingShopCount" type="int" public="false" private="false" save="true" desc="记录浴血沙场连续击败30名玩家"/>
		<property name="BattleWinCount" type="int" public="false" private="false" save="true" desc="记录实时战场连胜20次及以上"/>
		<property name="JewelryContinueCollect" type="int" public="false" private="false" save="true" desc="记录连续15次获得四级宝珠并成功提交奖励"/>
	</properties>
	<records>
		<!-- 成就表 -->
		<record name="achieve_process_rec" cols="4" maxrows="2048" save="true" public="false" private="true" desc="成就表">
			<column type="dword" desc="成就编号"/>
			<column type="int64" desc="数字(如已猎杀数目)"/>
			<column type="byte" desc="成就状态"/>
			<column type="byte" desc="是否是正在执行的子成就"/>
		</record>
		<!-- 成就领奖表 -->
		<record name="achieve_reward_rec" cols="1" maxrows="500" save="true" public="false" private="true" desc="成就领奖表">
			<column type="dword" desc="成就点"/>
		</record>

	</records>
</object>