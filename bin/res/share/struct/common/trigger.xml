<!--游戏对象属性定义模版文件-->
<object>
    <properties />
    <records>
        <!--触发器表-->
        <record name="TriggerRecord" cols="2" maxrows="32" save="false" public="false" private="true" desc="触发器表格">
            <column type="word" desc="触发器事件编号" />
            <column type="word" desc="触发器执行器编号" />
        </record>

		<!--触发器心跳表-->
		<record name="TriggerHeartbeatRecord" cols="8" maxrows="32" save="false" public="false" private="false" desc="触发器心跳表">
			<column type="word" desc="index"/>
			<column type="string" desc="name"/>
			<column type="object" desc="self"/>
			<column type="object" desc="target"/>
			<column type="word" desc="proctype"/>
			<column type="word" desc="procid"/>
            <column type="word" desc="start"/>
			<column type="word" desc="end"/>			
		</record>

		<!--触发器心跳队列表-->
		<record name="TriggerHeartBeatQueueRec" cols="9" maxrows="16" save="false" public="false" private="false" desc="触发器心跳队列表">
			<column type="string" desc="name"/>
			<column type="object" desc="self"/>
			<column type="object" desc="target"/>
			<column type="word" desc="proctype"/>
			<column type="word" desc="procid"/>
			<column type="word" desc="beat time (second)"/>
			<column type="word" desc="beat count"/>			
            <column type="word" desc="start"/>
			<column type="word" desc="end"/>
		</record>
    </records>
</object>
