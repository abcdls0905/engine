<?xml version="1.0" encoding="utf-8" ?>
<object>
    <properties>
        <property name="PortalPackID" type="string" public="false" private="false" save="false" realtime="" desc="传送包ID" />
        <property name="LimitCond" type="int" public="false" private="false" save="false" realtime="" desc="限制条件(条件包)" />
		<property name="MinSpringDistance" type="float" public="true" private="true" save="false" realtime="" desc="触发生效距离" />
        <property name="MaxSpringDistance" type="float" public="true" private="true" save="false" realtime="" desc="触发失效距离" />
		<property name="BornBuff" type="string" public="false" private="false" save="false" realtime="" desc="出生buff"/>
        <property name="SelfForceID" type="dword" public="true" private="false" save="false" desc="势力归属"/>
    </properties>

    <!-- 表属性 -->
    <records>
        <!-- 势力关系表 -->
        <record name="ForceRec" cols="2" maxrows="64" save="false" public="false" private="false" desc="势力关系表">
            <column type="word" desc="势力ID" />
            <column type="word" desc="关系" />
        </record>
    </records>
    <!--从其他文件加载属性-->
    <includes>
        <path name="struct\rule\fighter_data.xml" desc="战斗对象相关" />
    </includes>
</object>
