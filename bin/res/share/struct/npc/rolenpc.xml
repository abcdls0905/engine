<?xml version="1.0" encoding="gb2312"?>
<!--英雄类NPC基础属性-->
<object>
	<internals>
		<property name="VisualRange" type="float" public="true" private="true" save="false" desc="视野半径"/>
		<property name="Type" type="byte" public="true" private="true" save="false" realtime="" desc="类型: TYPE_NPC"/>
	</internals>
	<!-- 基本属性 -->
	<properties>
		<property name="ConfigID" type="string" public="true" private="true" save="true" desc="NPC ID"/>
		<property name="Resource" type="string" public="true" private="true" save="false" desc="资源"/>
		<property name="Level" type="int" public="true" private="true" save="true" realtime="true" desc="npc的等级"/>
		<property name="Height" type="float" public="true" private="true" save="false" realtime="" desc="模型高度"/>
		<property name="Scale" type="string" public="true" private="false" save="false" realtime="" desc="模型缩放比例"/>
		<property name="RoleIndex" type="int" public="true" private="true" save="true" desc="角色索引"/>
		<property name="AutoControl" type="int" public="true" private="false" save="false" desc="自动战斗"/>
		<property name="Transparency" type="int" public="true" private="false" save="false" realtime="" desc="透明度"/>
		<property name="FightID" type="string" public="true" private="true" save="false" realtime="" desc="战斗包ID"/>
		<property name="SkillList" type="string" public="true" private="true" save="false" realtime="" desc="战斗包ID"/>
		<property name="Price" type="int" public="true" private="true" save="true" desc="rolenpc价格"/>
		<property name="RoleMaster" type="object" public="true" private="true" save="true" realtime="" desc="角色主人"/>
		<property name="RoleChildIndex" type="byte" public="true" private="true" save="true" realtime="true" desc="相对于自己角色主人的孩子索引(1: 主控, 2: 附属1, 3: 附属2)"/>
		<property name="RoleChildPos" type="int" public="false" private="false" save="true" realtime="" desc="在自己角色主人的RoleNpcBox中的位置"/>
		<property name="PlayerExp" type="int" public="true" private="true" save="true" realtime="" desc="当前经验值"/>
		<property name="PlayerUpgradeExp" type="int" public="false" private="true" save="false" realtime="" desc="升级需求经验值"/>
		<property name="BattleAbility" type="int" public="true" private="true" save="false" realtime="" desc="战斗力"/>
		<property name="RoleNpcType" type="int" public="true" private="true" save="true" realtime="" desc="rolenpc类别"/>
		<property name="StepLevel" type="int" public="true" private="true" save="true" realtime="true" desc="进阶"/>
		<property name="AutoLockTarget" type="object" public="true" private="true" save="false" realtime="" desc="攻击自动锁定对象"/>
		<property name="Job" type="int" public="true" private="true" save="true" desc="角色职业"/>
		<property name="Race" type="int" public="true" private="true" save="true" desc="角色种族"/>
		<property name="Sex" type="int" public="true" private="true" save="true" desc="角色性别"/>
		<property name="AttackType" type="int" public="true" private="true" save="true" desc="角色攻击类型"/>
		<property name="RolePole" type="int" public="true" private="true" save="true" desc="四象，1，青龙。2，玄武。3，白虎。4，朱雀"/>
		<property name="TaichiSkillEffect" type="string" public="true" private="true" save="true" desc="太极技释放时特效"/>
		<property name="TaichiEffectTime" type="float" public="true" private="true" save="true" desc="太极技特效飞行时间"/>
		<property name="CanCrossNPC" type="byte" public="true" private="true" save="false" realtime="" desc="是否可穿怪"/>
		<property name="RoleTitleId" type="int" public="false" private="false" save="false" desc="当前称号"/>
		<property name="BeTaunted" type="byte" public="true" private="true" save="false" desc="是否被嘲讽"/>
		<property name="Camp" type="byte" public="true" private="true" save="true" desc="阵营（1、红方，2、蓝方）"/>
		<property name="EffectState" type="int" public="false" private="true" save="false" realtime="" desc="技能效果状态"/>
		<property name="CurrentHP" type="int" public="false" private="true" save="true" realtime="" desc="当前血量，用于野外场景"/>
		<property name="UseMount" type="int" public="true" private="true" save="true" realtime="true" desc="是否骑上坐骑"/>
		<property name="MountID" type="string" public="true" private="true" save="true" realtime="" desc="坐骑ID"/>
		<property name="VisibleState" type="byte" public="true" private="true" save="false" desc="隐身状态"/>
		<property name="SkillAwakePos1" type="byte" public="false" private="false" save="false" desc="技能1觉醒等级"/>
		<property name="SkillAwakePos2" type="byte" public="false" private="false" save="false" desc="技能2觉醒等级"/>
		<property name="SkillAwakePos3" type="byte" public="false" private="false" save="false" desc="技能3觉醒等级"/>
		<property name="SuitTotalUpgradeLevel" type="int" public="false" private="true" save="false" desc="进入本地副本时计算套装总强化等级"/>
		<property name="ActiveWingFashion" type="string" public="true" private="true" save="true" 		Desc="翅膀时装"/>
  </properties>
	<records>
		<record name="center_tattoo_prop_rec" cols="3" maxrows="10" save="true" public="false" private="true" realtime="true" desc="核心符文属性刷新表">
			<column type="string" desc="受影响的rolenpc"/>
			<column type="string" desc="核心符文ID"/>
			<column type="dword" desc="核心符文属性"/>
		</record>
		<record name="pet_rec" cols="1" maxrows="8" save="false" public="false" private="false" realtime="true" desc="宠物列表">
			<column type="object" desc="宠物persistid"/>
		</record>
		<record name="equip_rec" cols="3" maxrows="11" save="true" public="false" private="true" realtime="true" desc="装备列表">
			<column type="dword" desc="装备容器中index"/>
			<column type="string" desc="装备configid"/>
			<column type="string" desc="装备信息"/>
		</record>
		<record name="skill_rec" cols="2" maxrows="1024" save="true" public="false" private="true" realtime="true" desc="技能列表">
			<column type="string" desc="技能configid"/>
			<column type="dword" desc="技能等级"/>
		</record>
		<record name="taichi_skill_rec" cols="2" maxrows="1024" save="true" public="false" private="true" realtime="true" desc="技能列表">
			<column type="string" desc="技能configid"/>
			<column type="dword" desc="技能等级"/>
		</record>
		<record name="show_buff_overlay" cols="2" maxrows="8" save="false" public="true" private="true" realtime="false" desc="BUFF叠加表">
			<column type="dword" desc="buff index"/>
			<column type="dword" desc="层数"/>
		</record>
		<record name="tattoo_rec" cols="3" maxrows="16" save="true" public="false" private="true" realtime="true" desc="符文列表">
			<column type="byte" desc="符文容器中index"/>
			<column type="string" desc="符文configid"/>
			<column type="string" desc="符文信息"/>
		</record>
		<record name="fashion_rec" cols="3" maxrows="512" save="true" public="false" private="true" realtime="true" desc="装备列表">
			<column type="dword" desc="时装容器中index"/>
			<column type="string" desc="时装configid"/>
			<column type="string" desc="时装信息"/>
		</record>
		<record name="modify_prop_self" cols="2" maxrows="1000" save="false" public="true" private="true" desc="rolenpc对自身属性的影响">
			<column type="string" desc="修改的属性名"/>
			<column type="dword" desc="修改的属性值"/>
		</record>
		<record name="exchange_equip_rec" cols="2" maxrows="20" save="false" public="true" private="true" desc="一键换装表，存放背包里可换的最好装备">
			<column type="byte" desc="装备栏中的位置"/>
			<column type="byte" desc="背包栏中的位置"/>
		</record>
		<record name="fetter_addition_rec" cols="4" maxrows="400" save="false" public="false" private="true" desc="羁绊附加表">
			<column type="string" desc="修改的属性名"/>
			<column type="float" desc="影响的百分比"/>
			<column type="dword" desc="影响的固定属性"/>
			<column type="dword" desc="增加值"/>
		</record>
		<record name="element_damage_cd" cols="4" maxrows="6" save="false" public="false" private="false" desc="元素伤害CD表">
			<column type="byte" desc="触发元素伤害ID，冰火雷毒土风，如果策划再增加新元素，行数要修改"/>
			<column type="int64" desc="上一次触发元素伤害的时间，保存的是系统ticks"/>
			<column type="int64" desc="对应元素伤害的CD，策划效果包310配了，但是操蛋的是触发的时候就要计算"/>
			<column type="int64" desc="打出伤害的技能uuid"/>
		</record>
		<record name="player_fight_prop_compare_supreme_rec" cols="2" maxrows="300" save="false" public="false" private="false" desc="至尊切磋中装备相关的属性值。获得装备 灵魂连接 称号 时装 炼星 增加的 最大等级属性">
			<column type="string" desc="属性名"/>
			<column type="int" desc="属性值"/>
		</record>
	</records>
	<includes>
		<path name="struct\player\player_fight.xml" desc="rolenpc战斗相关属性"/>
		<path name="struct\rule\shortcut.xml" desc="技能栏相关属性"/>
		<path name="struct\rule\visequip.xml" desc="可视装备"/>
		<path name="struct\rule\visfashion.xml" desc="时装装备"/>
		<path name="struct\rule\motion.xml" desc="N移动属性"/>
		<path name="struct\rule\common\basectrlprop.xml" desc="控制属性"/>
		<path name="struct\ai\ai_npc_data.xml" desc="AI属性"/>
		<path name="struct\ai\ai_motion.xml" desc="AI属性"/>
		<path name="struct\rule\zhenying.xml" desc="阵营"/>
	</includes>
</object>
