<!--每项Buffer的属性数据，附加IDMATCH.xml-->
<object>
    <internals>
        <property name="LifeTime" type="dword" public="false" private="false" save="true" desc="持续时间(ms) 如果为0,则持久有效" />
    </internals>
    <!-- 基本属性 -->
    <properties>
        <!-- BUFFER的固定属性 -->
        <property name="ConfigID" type="string" public="true" private="true" save="false" desc="技能编号" />
        <property name="CurBufferUUID" type="int64" public="false" private="false" save="false" desc="当前正在使用buffer的64位唯一标志" />
        <property name="CurOverlays" type="dword" public="false" private="false" save="false" desc="buff 当前的叠加层数" />
        <property name="Level" type="word" public="false" private="false" save="true" realtime="false" desc="级别" />
        <property name="UniqueID" type="string" public="false" private="false" save="true" realtime="" desc="唯一编号" />
        
        <!-- BUFFER的施加来源属性 -->
        <property name="Sender" type="object" public="false" private="false" save="false" desc="BUFFER的施加者" />
        <property name="Skill" type="object" public="false" private="false" save="false" desc="产生BUFFER的技能" />
        <property name="MainBuffer" type="object" public="false" private="false" save="false" desc="子buffer对应的主buffer" />
        
        <!-- BUFFER的过程管理属性 -->
        <property name="AddTime" type="int64" public="false" private="false" save="true" desc="BUFFER添加的时间 单位s(time_t)" />
        <property name="Active" type="dword" public="false" private="false" save="true" desc="BUFFER是否有效" />
        <property name="Suspend" type="dword" public="false" private="false" save="true" desc="BUFFER是否暂停" />
        <property name="Counter" type="dword" public="false" private="false" save="true" desc="BUFFER已计次的次数" />
        <property name="Timer" type="dword" public="false" private="false" save="true" desc="BUFFER已存在的时间 单位ms" />

        <property name="ChainTimes" type="int" public="false" private="false" save="false" desc="当前弹射次数"/>
        <property name="BufferInfoIndex" type="int" public="false" private="false" save="false" desc="BUFFER记录的序号"/>

        <!-- 技能对象加载时需要设置-->
        <property name="BufferBasePackRow" type="dword" public="false" private="false" save="false" desc="BUFFER基础数据包索引行号用于查询加速" />
        <property name="MainPackRow" type="dword" public="false" private="false" save="false" desc="BUFFER主包索引行号用于查询加速" />
        <property name="DisplayPackRow" type="dword" public="false" private="false" save="false" desc="BUFFER表现包索引行号用于查询加速" />

        <!-- 配置文件中加载-->
        <property name="PropModifyPackID" type="int" public="false" private="false" save="false" desc="BUFFER修正包ID" />
        <property name="EventPackID" type="string" public="false" private="false" save="false" desc="事件包ID" />
        <property name="ClearPackID" type="string" public="false" private="false" save="false" desc="清除包ID" />
        <property name="ImmunityPackID" type="string" public="false" private="false" save="false" desc="免疫包ID" />
        
        <!-- 防止加载对象时报错误日志 -->
        <property name="Buff_Main" type="string" public="false" private="false" save="false" desc="BUFFER数据ID" />
        <property name="Buff_Display" type="string" public="false" private="false" save="false" desc="艺术表现ID" />
    </properties>
    <!-- 表属性 -->
    <records>
        <record name="overlay_buff_time_rec" cols="2" maxrows="64" save="false" public="false" private="false" desc="叠加buffer时间列表，有需求要保存这个表">
            <column type="word" desc="buffer已经过去的时间" />
            <column type="int64" desc="buffer添加的事件的时间(服务器时间 ms)" />
        </record>
        <record name="TempChainRec" cols="1" maxrows="10" save="false" public="false" private="false" desc="弹射记录表">
            <column type="object" desc="已经被弹过的目标"/>
        </record>
        <record name="around_relation_rec" cols="1" maxrows="128" save="false" public="false" private="false" desc="周围对象关系表">
            <column type="object" desc=""/>
        </record>
    </records>
</object>
