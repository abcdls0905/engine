<!--技能的属性数据-->
<object>
    <!-- 基本属性 -->
    <properties>
        <property name="ConfigID" type="string" public="false" private="true" save="false" desc="技能编号，客户端逻辑休修改，不用查网络对象的id" />
        <property name="Level" type="dword" public="false" private="true" save="true" realtime="false" desc="级别" />
        <property name="BaseLevel" type="dword" public="false" private="true" save="true" realtime="false" desc="基础等级" />
        <property name="AddLevel" type="dword" public="false" private="true" save="false" realtime="false" desc="附加等级" />
        
        <property name="CurSkillUUID" type="int64" public="false" private="false" save="false" desc="当前正在使用技能的64位唯一标志" />
        
        <property name="CurTargetCount" type="dword" public="false" private="false" save="false" desc="当前命中目标数量" />
        <property name="HurtCount" type="dword" public="false" private="false" save="false" desc="技能总命中打击次数,用于计算伤害" />
        <property name="CurHits" type="dword" public="false" private="false" save="false" desc="技能当前打击次数" />
        <property name="CurSkillOrient" type="float" public="false" private="false" save="false" realtime="false" desc="当前的技能朝向" />
        <property name="CurTouchOrient" type="float" public="false" private="false" save="false" realtime="false" desc="当前的摇杆朝向" />
        <property name="TalentSrcSkillID" type="string" public="false" private="false" save="true" desc="天赋原始技能ID" />
        <property name="TalentLevelSkillID" type="string" public="false" private="false" save="true" desc="天赋等级对应技能ID" />
		
        <!-- 技能对象加载时需要设置-->
        <property name="SkillBasePackRow" type="dword" public="false" private="false" save="false" desc="技能基础数据包索引行号用于查询加速" />
        <property name="MainPackRow" type="dword" public="false" private="false" save="false" desc="技能主包索引行号用于查询加速" />
        <property name="DisplayPackRow" type="dword" public="false" private="false" save="false" desc="技能表现包索引行号用于查询加速"/>
        
        <!-- 配置文件中加载-->
        <property name="FormulaPackID" type="string" public="false" private="false" save="false" desc="公式包ID" />
        <property name="ConsumePackID" type="string" public="false" private="false" save="false" desc=" 消耗包ID" />
        <property name="EventPackID" type="string" public="false" private="false" save="false" desc=" 事件包ID" />
        <property name="RangePackID" type="string" public="false" private="false" save="false" desc=" 射程包ID" />
        <property name="CoolDownPackID" type="string" public="false" private="false" save="false" desc=" 冷却包ID" />
        <property name="UpgradePackID" type="string" public="false" private="false" save="false" desc="升级包ID" />
        
        <!-- 防止加载对象时报错误日志-->
        <property name="Skill_Main" type="string" public="false" private="false" save="false" desc="技能数据ID" />
        <property name="Skill_Display" type="string" public="false" private="false" save="false" desc="技能表现ID" />
    </properties>
    <!-- 表属性 -->
    <records>
		<record name="skill_uuid_rec" cols="6" maxrows="32" save="false" public="false" private="false" desc="保存技能锁定目标记录表">
        <column type="int64" desc="技能UUID" />
		<column type="object"   desc="锁定对象" />
		<column type="int64"   desc="创建时间" />
		<column type="dword"   desc="当前技能命中次数" />
		<column type="dword"   desc="当前技能命中次序" />
		<column type="dword"   desc="技能持续效果" />
		</record>
        <record name="skill_transport_info_rec" cols="5" maxrows="3" save="false" public="false" private="false" desc="保存技能锁定目标记录表">
            <column type="int64" desc="种子结束时刻" />
            <column type="float" desc="种子位置点x" />
            <column type="float" desc="种子位置点y" />
            <column type="float" desc="种子位置点z" />
            <column type="byte" desc="该位置点剩余传送次数，次数为0时无效，该行记录将在心跳内删除" />
		</record>
    </records>
</object>
