<?xml version="1.0" encoding="gb2312" ?>
<!--AI移动相关-->
<object>
    	<!-- 基本属性 -->
    <properties>
		<property name="AIMotioning" type="dword" public="false" private="false" save="false" desc="AIMotion行动中" />

		<property name="SmoothRotate" type="dword" public="false" private="false" save="false" realtime="false" desc="是否平滑转向"/>
		<property name="PathFinding" type="int" public="false" private="false" save="false" desc="是否寻路中"/>
		<property name="FindPathX" type="float" public="false" private="false" save="false" desc="当前目标点X"/>
		<property name="FindPathZ" type="float" public="false" private="false" save="false" desc="当前目标点Z"/>
    </properties>
	<records>
		<record name="AIFindPathRec" cols="3" maxrows="512" save="flase" public="false" private="false" desc="寻路表">
			<column type="float" desc="x"/>
      			<column type="float" desc="y"/>
			<column type="float" desc="z"/>
		</record>
	</records>
	<includes>
		 <path name="struct\rule\motion.xml" desc="N移动属性" />
	</includes>
</object>
