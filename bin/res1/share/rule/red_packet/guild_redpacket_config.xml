<!-- 公会红包配置 -->
<!-- RefreshCD - 红包刷新CD(秒) -->
<!-- RobCD - 抢红包CD(秒) -->
<!-- ID - 红包唯一标示 -->
<!-- Price - 红包价格（单位RMB） -->
<!-- Count - 每次购买红包的数量 -->
<!-- FreeCharm - 免费点赞可获得的魅力值 -->
<!-- BuyCharm - 花费钻石点赞可获得的魅力值 -->
<!-- SendCharm - 发红包可获得的魅力值 -->
<!-- ItemPacket - 红包内物品包id -->
<Object>
	<Property Section="TotalConfig" RefreshCD="300" RobCD="1200"/>
	<Property Section="RedPacket_1" ID="RedPacket_1_1" Price="1" Count="1" FreeCharm="2" BuyCharm="4" SendCharm="2" ItemPacket="hb_cl_1"/>
	<Property Section="RedPacket_2" ID="RedPacket_1_2" Price="30" Count="10" FreeCharm="8" BuyCharm="16" SendCharm="80" ItemPacket="hb_hb_2"/>
	<Property Section="RedPacket_3" ID="RedPacket_1_3" Price="648" Count="10" FreeCharm="400" BuyCharm="800" SendCharm="4000" ItemPacket="hb_qt_3"/>
</Object>
