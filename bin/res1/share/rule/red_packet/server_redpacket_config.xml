<!-- 被2011 rel. 2 sp1 (Lonely Knight) 使用XMLSpy vLK编辑的 (http://www.altova.com) by -->
<!-- 全服红包配置 -->
<!-- RefreshCD - 红包刷新CD(秒) -->
<!-- RobCD - 抢红包CD(秒) -->
<!-- ID - 红包唯一标示 -->
<!-- Price - 红包价格（单位RMB） -->
<!-- Count - 每次购买红包的数量 -->
<!-- FreeCharm - 免费点赞可获得的魅力值 -->
<!-- BuyCharm - 花费钻石点赞可获得的魅力值 -->
<!-- SendCharm - 发红包可获得的魅力值 -->
<!-- ItemPacket - 红包内物品包id -->
<!-- NeedBroadcast - 发红包时是否需要广播（0-不需要；1-需要） -->
<Object>
	<Property Section="TotalConfig" RefreshCD="300" RobCD="1200"/>
	<Property Section="RedPacket_1" ID="RedPacket_2_1" Price="30" Count="30" FreeCharm="2" BuyCharm="4" SendCharm="60" NeedBroadcast="0" ItemPacket="hb_cl_1"/>
	<Property Section="RedPacket_2" ID="RedPacket_2_2" Price="98" Count="20" FreeCharm="20" BuyCharm="40" SendCharm="400" NeedBroadcast="0" ItemPacket="hb_hb_2"/>
	<Property Section="RedPacket_3" ID="RedPacket_2_3" Price="328" Count="5" FreeCharm="300" BuyCharm="600" SendCharm="1500" NeedBroadcast="1" ItemPacket="hb_qt_2"/>
</Object>
