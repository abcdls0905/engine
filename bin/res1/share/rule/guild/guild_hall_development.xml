<!-- CapitalType-建设需要金钱类别（1-金币；2-钻石） -->
<!-- VipLevel-建设需要达到的VIP等级-->
<!-- CapitalValue-建设需要金钱数量 -->
<!-- AddDevelopment-增加公会建设度 -->
<!-- AddDevote-增加个人贡献值 -->
<Object>
  <Property ID="0" CapitalType="1" VipLevel="0" CapitalValue="20000" AddDevelopment="200" AddDevote="200"/>
  <Property ID="1" CapitalType="2" VipLevel="0" CapitalValue="20" AddDevelopment="600" AddDevote="400"/>
  <Property ID="2" CapitalType="2" VipLevel="2" CapitalValue="200" AddDevelopment="1000" AddDevote="2000"/>
  <Property ID="3" CapitalType="2" VipLevel="5" CapitalValue="1000" AddDevelopment="3000" AddDevote="5000"/>
</Object>
