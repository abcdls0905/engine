<?xml version="1.0" encoding="utf-8" ?>
<object>
    <internals />
    <properties>
        <property name="AttackNpcType" type="int" public="true" private="false" save="false" realtime="" desc="攻击npc类型(0: 普通  1: 精英  2: 稀有  3: boss)" />
        <property name="BornAction" type="string" public="false" private="false" save="false" realtime="" desc="出生动作" />
        <property name="BornProtectTime" type="int" public="false" private="false" save="false" desc="保护时间（可跟进动作时间来设定）"/>
        <property name="FightAction" type="string" public="false" private="true" save="false" realtime="" desc="npc进入战斗动作" />
        <property name="FightProtectTime" type="int" public="false" private="true" save="false" realtime="" desc="npc进入战斗保护时间" />
		<property name="IsWorldBoss" type="int" public="false" private="false" save="false" realtime="" desc="是否是世界BOSS" />
		<property name="Camp" type="int" public="true" private="false" save="false" realtime="" desc="战场阵营号"/>
		<property name="OwnerCamp" type="int" public="true" private="false" save="false" realtime="" desc="战场所有者的阵营号"/>
		<property name="TempData" type="int" public="true" private="false" save="false" realtime="" desc="通用临时数据"/>
    </properties>
    <records>
	    <record name="battlewar_helpattack_rec" cols="2" maxrows="50" save="false" public="false" private="false" desc="被攻击者身上的助攻表">
			<column type="widestr" key="default" desc="攻击者名字"/>
			<column type="int64"   desc="攻击时间"/>
		</record>
	</records>
    <includes>
        <path name="struct\npc\npcproperty\awards.xml" desc="NPC死亡奖励属性" />
        <path name="struct\npc\npcproperty\fight.xml" desc="NPC战斗属性" />
        <path name="struct\npc\rule\modifybase.xml" desc="属性 技能 buff修正表" />
        <path name="struct\ai\ai_npc_data.xml" desc="AI属性" />
        <path name="struct\ai\ai_motion.xml" desc="AI属性" />
		<path name="struct\npc\verify\verify.xml" desc="NPC进阶配置" />
    </includes>
</object>
