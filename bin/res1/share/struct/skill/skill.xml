<!--技能的属性数据-->
<object>
	<!-- 基本属性 -->
	<properties>
		<property name="ConfigID" type="string" public="false" private="true" save="false" desc="技能编号，客户端逻辑休修改，不用查网络对象的id"/>"/>
		<property name="CurTargetCount" type="int" public="false" private="false" save="false" desc="当前命中目标数量"/>
		<property name="HurtCount" type="int" public="false" private="false" save="false" desc="技能总命中打击次数,用于计算伤害"/>
		<property name="CurHits" type="int" public="false" private="false" save="false" desc="技能当前打击次数"/>
		<property name="Level" type="int" public="true" private="true" save="true" realtime="true" desc="级别"/>
	    <property name="GraspLevel" type="int" public="false" private="false" save="true" desc="技能领悟的级别"/>
	    <property name="RarePassiveLevel" type="int" public="false" private="false" save="false" desc="稀有技能被动的级别"/>		
    	<property name="IsNormalAttack" type="byte" public="false" private="true" save="true" desc="是否是普通攻击技能:0不是，1是"/>
		<property name="CoolDownTime" type="int" public="false" private="false" save="false" desc="冷却时间,单位为ms"/>
		<property name="PetFrontSkill" type="int" public="false" private="true" save="true" desc="宠物上阵技能(标识)"/>
		<property name="CurSkillOrient" type="float" public="false" private="false" save="false" realtime="false" desc="当前的技能朝向"/>
		<property name="CurSkillPosX" type="float" public="false" private="false" save="false" realtime="false" desc="当前的技能坐标X"/>
		<property name="CurSkillPosZ" type="float" public="false" private="false" save="false" realtime="false" desc="当前的技能坐标Z"/>
        	<property name="CantUse" type="byte" public="false" private="false" save="false" desc="是否能够使用此技能:0能，1不能"/>
		<!-- 以下三个是为了防止加载保存，建议引擎解决预加载时屏蔽不需要的配置-->
        <property name="Description" type="string" public="false" private="false" save="false" desc="技能描述"/>
        <property name="Skill_Main" type="string" public="false" private="false" save="false" desc="技能主包"/>
        <property name="Skill_Display" type="string" public="false" private="false" save="false" desc="表现包"/>
	</properties>
</object>
