<!--Equip_Item的属性表，继承自Nim_Item，所有可装备的物品的基类-->
<object>
	<!-- 基本属性 -->
	<properties>
	
        <property name="AdvHP" type="Int" public="false" private="false" save="true" desc="每次进阶增长的血量"/>
        <property name="AdvAtk" type="Int" public="false" private="false" save="true" desc="每次进阶增长的攻击"/>
        <property name="StrengthenHP" type="Int" public="false" private="false" save="true" desc="每次强化增长的血量"/>
        <property name="StrengthenAtk" type="Int" public="false" private="false" save="true" desc="每次强化增长的攻击"/>
        <property name="AdvPropPackage" type="string" public="false" private="false" save="true" desc="进阶属性包"/>
        <property name="StrengthenPropPackage" type="string" public="false" private="true" save="true" desc="强化属性包"/>
        <property name="StrengthenCurLevel" type="Int" public="false" private="true" save="true" realtime="true" desc="当前强化等级"/>
        <property name="UpgradeCurLevel" type="Int" public="false" private="true" save="true" realtime="true" desc="当前进阶等级"/>
        <property name="StrengthenCurExp" type="Int" public="false" private="true" save="true" realtime="true" desc="当前强化经验"/>
        <property name="RemarkableGrooveNum" type="Int" public="false" private="true" save="true" realtime="true" desc="卓越进阶槽数"/>
        <property name="EquipType" type="string" public="false" private="true" save="false" desc="装备栏位类型, 见EquipDefine.ini文件中的定义"/>

        <property name="FightingCapacity" type="int" public="false" private="false" save="false" desc="装备战斗力"/>
		<property name="ElementLabel" type="string" public="false" private="false" save="false" desc="元素标签"/>
		<property name="SpecialLabel" type="string" public="false" private="false" save="false" desc="特殊标签"/>
		<property name="ColorLabel" type="int" public="false" private="false" save="false" desc="颜色标签"/>
		<property name="NameLabel" type="int" public="false" private="false" save="false" desc="装备标签"/>
        <property name="AwakeEquipPropName" type="string" public="false" private="true" save="true" realtime="true" desc="觉醒的属性名"/>
		<property name="GoldEquipQuality" type="int" public="false" private="true" save="true" realtime="true" desc="金装品质"/>
		<property name="GoldEquipPropPack" type="string" public="false" private="false" save="true" realtime="true" desc="金装属性包"/>
		<property name="FlyHP" type="Int" public="false" private="true" save="true" realtime="true" desc="翅膀飞升增长的血量"/>
        <property name="FlyAtk" type="Int" public="false" private="true" save="true" realtime="true"  desc="翅膀飞升增长的攻击"/>
        <property name="FlyCurLevel" type="Int" public="false" private="true" save="true" realtime="true" desc="当前飞升等级"/>
        <property name="EquipBufferID" type="string" public="false" private="true" save="true" realtime="true" desc="装备身上的BufferID"/>		
		<property name="WingTransformType" type="int" public="false" private="true" save="true" realtime="true" desc="翅膀蜕变类型"/>
		<property name="WingTransformLevel" type="int" public="false" private="true" save="true" realtime="true" desc="翅膀蜕变等级"/>
		<property name="RemarkableEquipTeam" type="string" public="false" private="true" save="false" realtime="false" desc="卓越领悟组"/>
		<property name="BaseAbilityBattle" type="string" public="false" private="true" save="false" realtime="false" desc="武神装备基础战斗"/>
		<property name="strenAbilityBattle" type="string" public="false" private="true" save="false" realtime="false" desc="武神装备强化后增加的战斗力"/>
		<property name="TaozhuangList" type="string" public="false" private="true" save="false" realtime="false" desc="套装列表"/>
		<property name="ValidGameTime" type="int" public="false" private="true" save="false" realtime="false" desc="有效的游戏时间"/>
		<property name="CoolDownTime" type="int" public="false" private="true" save="false" realtime="false" desc="冷却时间"/>
		<property name="SaveLogLevel" type="int" public="false" private="true" save="false" realtime="false" desc="保存日志等级"/>

	</properties>
	
	<!--策划要求装备的修正表必须保存不得重刷-->
	<records>
        <record name="OpenLockStrenRec" cols="7" maxrows="16" save="true" public="false" private="true" desc="强化解锁属性表">
            <column type="string" desc="属性名"/>
            <column type="DWORD" desc="属性值"/>
            <column type="DWORD" desc="解锁条件"/>
            <column type="DWORD" desc="是否为激活状态1为激活0为不激活即不能起到作用"/>
            <column type="DWORD" desc="是否阴影显示1为显示0为不显示，无论显示与否都不在装备上起作用"/>
            <column type="string" desc="说明文字ID"/>
            <column type="string" desc="说明文字ID"/>
        </record>
        <record name="OpenLockUpgradeRec" cols="7" maxrows="16" save="true" public="false" private="true" desc="进阶解锁属性表">
            <column type="string" desc="属性名"/>
            <column type="DWORD" desc="属性值"/>
            <column type="DWORD" desc="解锁条件"/>
            <column type="DWORD" desc="是否为激活状态1为激活0为不激活即不能起到作用"/>
            <column type="DWORD" desc="是否阴影显示1为显示0为不显示，无论显示与否都不在装备上起作用"/>
            <column type="string" desc="说明文字ID"/>
            <column type="string" desc="说明文字ID"/>
        </record>
		<record name="AwakePropRec" cols="7" maxrows="16" save="true" public="false" private="true" desc="觉醒属性表">
            <column type="string" desc="属性名"/>
            <column type="DWORD" desc="属性值"/>
			<column type="DWORD" desc="当前等级"/>
            <column type="DWORD" desc="每级提升值"/>
            <column type="DWORD" desc="最大等级"/>
            <column type="DWORD" desc="属性颜色"/>
            <column type="string" desc="属性名称ID"/>
        </record>
        <record name="RemarkablePropRec" cols="5" maxrows="16" save="true" public="false" private="true" desc="卓越属性表">
            <column type="string" desc="属性名"/>
            <column type="DWORD" desc="属性值"/>
            <column type="string" desc="属性类型"/>
            <column type="string" desc="相斥类型"/>
            <column type="DWORD" desc=" 是否加锁"/>
        </record>
        <record name="AwakePropDepositRec" cols="7" maxrows="16" save="true" public="false" private="true" desc="觉醒寄存表">
            <column type="string" desc="属性名"/>
            <column type="DWORD" desc="属性值"/>
			<column type="DWORD" desc="当前等级"/>
            <column type="DWORD" desc="每级提升值"/>
            <column type="DWORD" desc="最大等级"/>
            <column type="DWORD" desc="属性颜色"/>
            <column type="string" desc="属性名称ID"/>
        </record>
        <record name="PropModifyByTattoo" cols="3" maxrows="300" save="false" public="false" private="false" desc="符文属性刷新表">
            <column type="string" desc="属性名"/>
            <column type="float" desc="属性值"/>
            <column type="string" desc="来源"/>
        </record>
        <record name="PropTattooRuneWords" cols="3" maxrows="300" save="false" public="false" private="false" desc="符文属性刷新表">
            <column type="string" desc="属性名"/>
            <column type="float" desc="属性值"/>
            <column type="string" desc="属性ID"/>
        </record>
        <record name="OpenLockWflyRec" cols="7" maxrows="16" save="true" public="false" private="true" desc="翅膀飞升解锁属性表">
            <column type="string" desc="属性名"/>
            <column type="DWORD" desc="属性值"/>
            <column type="DWORD" desc="是否为激活状态1为激活0为不激活即不能起到作用"/> 
            <column type="DWORD" desc="飞升多少级解锁"/>
            <column type="string" desc="说明文字ID"/>
            <column type="string" desc="说明文字ID"/>
            <column type="string" desc="PackID"/>
          </record>
	</records>
	
	<!--从其他文件加载属性-->
	<includes>
		<path name="struct\rule\fight\fightpropdefined.xml" desc="战斗属性附加"/>
	</includes>	
</object>
