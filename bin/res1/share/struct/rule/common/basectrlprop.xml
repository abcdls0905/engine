<?xml version="1.0" encoding="gb2312"?>
<!--控制属性-->
<object>
    <!-- 基本属性 -->
    <properties>
        <property name="CantMove" type="byte" public="true" private="true" save="false" realtime="true" desc="是否能够移动 0:可移动 1:不能移动"/>
        <property name="CantRotate" type="byte" public="true" private="true" save="false" realtime="true" desc="是否能够转动 0:可转动 1:不能转动"/>
        <property name="CantAttack" type="byte" public="false" private="true" save="false" desc="是否能够攻击"/>
        <property name="CantBeAttack" type="byte" public="true" private="true" save="false" desc="是否能够被攻击 0:可被攻击 1：不可被攻击"/>
		<property name="CantBeKilled" type="byte" public="false" private="false" save="false" desc="是否能够被杀死"/>
        <property name="CantUseItem" type="byte" public="false" private="true" save="false" desc="是否能够使用道具"/>
        <property name="CantBeAttackMove" type="byte" public="false" private="false" save="false" realtime="true" desc="不受击飞击退击倒等影响"/>
		<property name="CantBeHitRotate" type="byte" public="true" private="true" save="false" realtime="true" desc="受击时转向攻击者"/>
        <property name="CantBeStiff" type="byte" public="false" private="false" save="false" desc="不受硬直影响"/>
		<property name="CanGaint" type="byte" public="true" private="true" save="false" desc="免疫控制效果"/>
        <property name="CanBreak" type="byte" public="true" private="true" save="false" desc="是否能破防"/>
        <property name="CanParry" type="byte" public="true" private="true" save="false" desc="是否正面防御"/>
		<property name="CantDoAction" type="byte" public="true" private="true" save="false" realtime="true" desc="不能做动作"/>
        <property name="CantUseSkill" type="byte" public="false" private="true" save="false" realtime="true" desc="是否能够使用技能"/>
        <property name="ForbidUseSkill" type="byte" public="false" private="true" save="false" realtime="true" desc="禁止使用技能（用于进入副本倒计时过程的禁用）"/>
		<property name="CantBeDamage" type="byte" public="false" private="false" save="false" realtime="true" desc="是否免疫伤害"/>
		<property name="IsConsume" type="byte" public="false" private="false" save="false" desc="使用技能是否产生消耗"/>  
		<property name="IsFlight" type="byte" public="true" private="true" save="false" realtime="true" desc="是否在漂浮状态"/>  
        <property name="CanBreakFloating" type="byte" public="true" private="true" save="false" realtime="false" desc="能否打断浮空状态"/>
        <property name="CantLeech" type="byte" public="false" private="false" save="false" realtime="false" desc="能否吸血"/>
        <property name="CantChaos" type="byte" public="false" private="true" save="false" desc="是否处于混乱状态"/>
		<property name="CantDrag" type="byte" public="true" private="true" save="false" realtime="false" desc="能否能够被抓取"/>
		<property name="IsEliminateEquipCD" type="byte" public="false" private="true" save="false" realtime="false" desc="是否能消除玩家装备引起的CD变化 0:不行 1:可以"/>
        <property name="ControlSetFlag" type="int" public="false" private="false" save="false" realtime="false" desc="用于标记stub逻辑中设置控制属性的代码位置，方便检测"/>
		<property name="CantPauseFrame" type="byte" public="false" private="true" save="false" realtime="true" desc="是否暂停动作帧"/>	
		<property name="CanUseNoramlSkill" type="byte" public="false" private="true" save="false" realtime="true" desc="是否能够使用普通攻击技能"/>
    </properties>

	<records>
		<record name="ctrl_prop_rec" cols="4" maxrows="100" save="false" public="false" private="true" desc="控制属性控制表">
			<column type="string" desc="表示的属性名称"/>
			<column type="int64" desc="结束时间" />
			<column type="byte" desc="属性修改值" />
			<column type="object" desc="buffer" />
		</record>
	</records>
</object>