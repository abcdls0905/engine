<!--当前玩家拥有的副本ID--><object>
	<!-- 基本属性 -->
	<properties>
        <property name="SweepTime" type="int64" public="false" private="false" save="true" realtime="true" desc="扫荡冷却到期时间" />
		<property name="CloneDifficulty" type="int" public="false" private="true" save="true" desc="副本难度(1-简单 2-困难)"/>	
		<property name="ProtoSceneID" type="int" public="false" private="true" save="true" realtime="true" desc="原形场景编号"/>
		<property name="EnterTimesJ" type="int" public="false" private="true" save="true" desc="精英副本调整次数"/>
		<property name="TeamsCloneTimes" type="int" public="false" private="false" save="true" desc="主线组队副本挑战次数"/>
		<property name="SpecialCloneTimes" type="int" public="false" private="false" save="true" desc="活动组队副本挑战次数"/>
	    <property name="CloneSceneShopRefreshDate" type="string" public="false" private="false" save="true" realtime="" desc="刷新副本商店时间"/>
		<property name="CloneDropNotExist" type="int" public="false" private="false" save="false" desc="副本掉落不存在"/>
		<property name="CloneDropRadio" type="int" public="false" private="true" save="false" realtime="true"  desc="副本掉落概率倍率"/>
		<property name="CloneExpRatio" type="int" public="false" private="false" save="false" realtime="false"  desc="本地副本经验加成"/>
		<property name="CloneExpRatioAdd" type="int" public="false" private="false" save="false" realtime="false"  desc="本地副本经验加成"/>
		<property name="LargeCloneEntryPlayerNum" type="int" public="false" private="false" save="false" realtime="false"  desc="多人团队副本进入人数"/>
		<property name="LargeCloneTeamCaptain" type="string" public="false" private="false" save="false" realtime="false"  desc="多人团队副本组队队长"/>
	</properties>
	<records>
		<record name="clone_info_record" cols="10" maxrows="512" save="true" public="false" private="true" desc="副本信息表">
			<column type="dword" desc="副本原型id"/>			
			<column type="dword" desc="进入次数"/>
			<column type="dword" desc="副本当前进度" vistype="hide"/>
			<column type="dword" desc="副本难度" vistype="hide"/>
			<column type="dword" desc="副本场景ID" vistype="hide"/>
			<column type="int64" desc="副本刷新时间" vistype="hide"/>
			<column type="string" desc="副本唯一UID" vistype="hide"/>
			<column type="dword" desc="是否开启" vistype="hide"/>
     		<column type="widestr" desc="副本拥有者" vistype="hide"/>
			<column type="dword" desc="副本类型(个人，队伍)" vistype="hide"/>      
		</record>
		<record name="clone_passed_record" cols="4" maxrows="512" save="true" public="false" private="true" desc="已经通过的副本表">
		  <column type="dword" desc="副本原型id"/>
		  <column type="dword" desc="副本难度"/>
		  <column type="dword" desc="星级"/>
		  <column type="dword" desc="通关时间"/>
		</record>
		<record name="clone_reset_record" cols="4" maxrows="512" save="true" public="false" private="true" desc="扫荡冷却时间和挑战次数重置次数">
		  <column type="dword" desc="副本原型id"/>
		  <column type="dword" desc="副本难度"/>
		  <column type="dword" desc="冷却时间被重置次数"/>
		  <column type="dword" desc="挑战次数被重置次数"/>
		</record>
		<record name="clone_unlock_record" cols="3" maxrows="100" save="true" public="false" private="true" desc="副本解锁表">
		  <column type="dword" desc="副本原型id"/>
		  <column type="string" desc="副本已解锁难度"/>
		  <column type="string" desc="副本解锁条件记录"/>
		</record>
		<record name="clone_reward_record" cols="3" maxrows="512" save="true" public="false" private="true" desc="章节通关评分记录">
		  <column type="dword" desc="章节id"/>
		  <column type="byte" desc="模式"/>
		  <column type="byte" desc="评价"/>
		</record>
		<record name="clone_get_record" cols="3" maxrows="512" save="true" public="false" private="true" desc="副本获得信息表">
		  <column type="dword" desc="物品类型"/>
		  <column type="string" desc="物品名称"/>
		  <column type="dword" desc="物品数量"/>
		</record>
		<record name="clone_teams_record" cols="4" maxrows="25" save="false" public="false" private="false" desc="组队副本队友信息">
		  <column type="string" desc="组队成员uid，第一个是队长"/>
		  <column type="byte" desc="职业"/>
		  <column type="dword" desc="战斗力"/>
		  <column type="dword" desc="等级"/>
		</record>
		<record name="clone_shop_rec" cols="5" maxrows="100" save="true" public="false" private="true" desc="战场巅峰商店表">
		    <column type="dword"  desc="物品编号" />
		    <column type="string" desc="物品ID" />
            <column type="dword"  desc="物品剩余个数" />
            <column type="dword"  desc="价格" />
             <column type="dword"  desc="是否刷新" />
        </record>
		<record name="clone_gold_live_sweep_rec" cols="3" maxrows="32" save="true" public="false" private="false" desc="历练副本扫荡最大收益记录表">
		    <column type="dword"  desc="副本原型id" />
		    <column type="dword" desc="获得金钱" />
			<column type="string" desc="获得物品(金钱道具和陨铁道具)" />
		</record>
	</records>
</object>
