<?xml version="1.0" encoding="utf-8" ?>
<object>
    <internals />
    <properties>
		<property name="VoyageEveryDayRefresh" type="int64" public="false" private="false" save="true" realtime="" desc="通天塔每日刷新时间" />
        <property name="VoyageBuildCount" type="int" public="false" private="true" save="true" realtime="" desc="当天建造次数" />
		<property name="VoyageHelpBuildCount" type="int" public="false" private="true" save="true" realtime="" desc="当天帮助建造次数" />
		<property name="VoyagePillageCount" type="int" public="false" private="true" save="true" realtime="" desc="当天掠夺次数" />
		<property name="VoyageFreeRefreshTTCount" type="int" public="false" private="true" save="true" realtime="" desc="当天免费刷新通天塔次数" />
		<property name="VoyageFreeRefreshPillageCount" type="int" public="false" private="true" save="true" realtime="" desc="当天免费刷新掠夺列表次数" />
		<property name="VoyageDiamondRefreshDistroyCount" type="int" public="false" private="false" save="true" realtime="" desc="钻石刷新摧毁列表次数" />
		<property name="VoyageNearestFightTime" type="int64" public="false" private="true" save="false" realtime="" desc="最近一次通天塔战斗还有多少秒开始" />
	</properties>
	<records>
		<record name="voyage_tt_rec" cols="2" maxrows="10" save="true" public="false" private="true" desc="通天塔表格">
			<column type="dword" desc="通天塔ID" />
			<column type="dword" desc="修建状态" />
		</record>
		<record name="voyage_enemy_rec" cols="4" maxrows="10" save="true" public="false" private="true" desc="仇人表格">
			<column type="string" desc="仇人的uid" />
			<column type="dword" desc="仇人的serverid" />
			<column type="dword" desc="仇人的状态" />
			<column type="widestr" desc="仇人的name" />
		</record>
		<record name="voyage_trends_rec" cols="3" maxrows="10" save="true" public="false" private="true" desc="动态列表">
			<column type="dword" desc="动态类型" />
			<column type="widestr" desc="动态信息参数1" />
			<column type="string" desc="动态信息参数2" />
		</record>
		<record name="voyage_destroy_rec" cols="8" maxrows="20" save="true" public="false" private="true" desc="可摧毁通天塔表格">
			<column type="string" desc="修建通天塔玩家uid" />
			<column type="widestr" desc="玩家name" />
			<column type="dword" desc="玩家roleindex" />
			<column type="dword" desc="通天塔id" />
			<column type="dword" desc="玩家serverid" />
			<column type="int64" desc="修建开始时刻" />
			<column type="byte" desc="通天塔状态" />
			<column type="int64" desc="剩余时间" />
		</record>
		<record name="voyage_fight_rec" cols="4" maxrows="10" save="true" public="false" private="true" desc="作战计划表格">
			<column type="dword" desc="作战时间-小时" />
			<column type="dword" desc="作战时间-分钟" />
			<column type="dword" desc="作战类型" />
			<column type="string" desc="通天塔修建者的uid" />
		</record>
	</records>
	<includes />
</object>
