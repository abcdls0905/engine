<!--技能使用相关的属性(npc,玩家角色共用)-->
<object>
    <!-- 基本属性 -->
    <properties>
        <property name="CurSkill" type="object" public="false" private="false" save="false" desc="当前正在使用的技能"/>
        <property name="CurSkillUUID" type="int64" public="false" private="false" save="false" desc="当前正在使用技能的64位唯一标志"/>
        <property name="CurSkillType" type="int" public="false" private="false" save="false" realtime="false" desc="当前正在使用的技能方式"/>
        <property name="CurSkillTarget" type="object" public="false" private="false" save="false" realtime="false" desc="当前的技能目标"/>
        <property name="CurSkillTargetX" type="float" public="false" private="false" save="false" realtime="false" desc="当前的技能坐标"/>
        <property name="CurSkillTargetY" type="float" public="false" private="false" save="false" realtime="false" desc="当前的技能坐标"/>
        <property name="CurSkillTargetZ" type="float" public="false" private="false" save="false" realtime="false" desc="当前的技能坐标"/>
        
        <property name="UseBeforeMoveSelfPos" type="byte" public="false" private="false" save="false" desc="是否要使用位移前的坐标"/>
        <property name="BeforeMoveSelfPosX" type="float" public="false" private="false" save="false" desc="自身在位于前的坐标"/>
        <property name="BeforeMoveSelfPosY" type="float" public="false" private="false" save="false" desc="自身在位于前的坐标"/>
        <property name="BeforeMoveSelfPosZ" type="float" public="false" private="false" save="false" desc="自身在位于前的坐标"/>
        <property name="BeforeMoveSelfPosO" type="float" public="false" private="false" save="false" desc="自身在位于前的坐标"/>
	<property name="SkillTimeRatio" type="int" public="false" private="true" save="false" desc="技能时间倍率"/>

</properties>
    
    <!-- 表属性 -->
    <records>
        <record name="cooldown_rec" cols="4" maxrows="32" save="false" public="false" private="false" desc="冷却表">
            <column type="dword" desc="冷却分类ID" />
            <column type="int64" desc="冷却开始时间，现实64位时间数据" />
            <column type="int64" desc="冷却结束时间，现实64位时间数据" />
            <column type="word" desc="冷却大分类ID,更上一层的分类" />
        </record>
      <record name="disable_effect_funs_rec" cols="2" maxrows="64" save="false" public="false" private="false" desc="被禁用的高绩效列表">
          <column type="object" desc="配置了禁用指定高绩效果功能号的buffer对象" />
          <column type="word" desc="被禁用的高绩效果功能号" />
      </record>
		<record name="buffer_modify_temporary_damage_rec" cols="3" maxrows="50" save="false" public="false" private="false" desc="战斗修正临时属性表">
            <column type="object" desc="修正的Buffer对象ID"/>			
			<column type="byte" desc="修正的类别 1.额外的附加伤害值 2.免疫 3.分担伤害 4.吸收伤害"/>
            <column type="float" desc="修正的百分比"/>
        </record>		
    </records>
    
    <!--从其他文件加载属性-->
    <includes>
    </includes>
</object>