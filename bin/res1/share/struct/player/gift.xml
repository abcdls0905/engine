<?xml version="1.0" encoding="gb2312"?>

<object>
    <properties>
	    <property name="ServerCurrentDayTime" type="string" public="false" private="true" save="false" realtime="" desc="服务器当前日期" />   
	    <property name="ShareWithFriendsTimes" type="dword" public="false" private="true" save="true" realtime="true" desc="记录分享到朋友圈的次数" />   
        <property name="ShareWithFriendsMoment" type="int64" public="false" private="true" save="true" realtime="true" desc="记录上次分享到朋友圈的时刻" />
		<property name="PhoneBindState" type="int" public="false" private="true" save="true" realtime="true" desc="绑定手机状态（0未绑定；1已绑定）" />
		<property name="PhoneBindGiftState" type="int" public="false" private="true" save="true" realtime="true" desc="绑定手机状态礼品领取状态（0未领取；1已领取）" />
		<property name="SignInMoment" type="int64" public="false" private="true" save="true" realtime="true" desc="月签每日签到时刻" />
		<property name="SignInVersion" type="int" public="false" private="false" save="true" realtime="" desc="月签版本号" />
		<property name="AppStoreGradeState" type="int" public="false" private="false" save="true" realtime="" desc="appstore评分状态" />
		<property name="MaterialCount" type="int" public="false" private="true" save="true" realtime="" desc="本次材料礼包剩余天数" />
		<property name="MaterialBeginTime" type="double" public="false" private="false" save="true" realtime="" desc="本次材料礼包开始时间" />
		<property name="GetMaterialTime" type="int64" public="false" private="true" save="true" realtime="" desc="最后一次领取材料礼包的时间" />
	</properties>
    <records>
        <record name="level_gift_rec" cols="2" maxrows="100" save="true" public="false" private="true" desc="等级礼包表">
            <column type="dword" desc="解锁等级" />
            <column type="dword" desc="是否已领取"/>
        </record>
		<record name="new_server_gift_rec" cols="2" maxrows="100" save="true" public="false" private="true" desc="新服礼包表">
            <column type="dword"  desc="领取个数" />
			<column type="string" desc="领取日期"/>
        </record>
		<record name="online_gift_rec" cols="2" maxrows="100" save="true" public="false" private="true" desc="在线礼包">
			<column type="dword"  desc="当前剩余秒数"/>
			<column type="dword"  desc="是否已领取" />
        </record>
		<record name="everyday_gift_rec" cols="2" maxrows="100" save="true" public="false" private="true" desc="签到礼包">
			<column type="dword"   desc="解锁天数"/>
			<column type="string"  desc="领取的日期" />
        </record>
		<record name="eai_gift_rec" cols="4" maxrows="100" save="false" public="false" private="true" desc="eai发放的奖励包">
			<column type="dword"   desc="礼包id"/>
			<column type="string"  desc="奖励物品名称" />
			<column type="dword"   desc="奖励物品数量"/>
			<column type="widestr" desc="礼包描述信息" />
		</record>
		<record name="sign_in_rec" cols="3" maxrows="100" save="true" public="false" private="true" desc="月签表">
			<column type="word" desc="第几天"/>
			<column type="word" desc="签到时VIP等级"/>
			<column type="byte" desc="是否是补签"/>
		</record>
		<record name="invite_gift_rec" cols="2" maxrows="100" save="true" public="false" private="true" desc="邀请有礼礼包表">
			<column type="dword" desc="礼包在配置中的ID" />
			<column type="dword" desc="是否已领取"/>
        </record>
	</records>
</object>