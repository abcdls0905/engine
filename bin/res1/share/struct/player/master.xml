<?xml version="1.0" encoding="utf-8"?>

<object>
	<properties>
        <property name="Master_ID" type="string" public="false" private="true" save="true" realtime="false" desc="师门id"/>
        <property name="Master_Switch" type="int" public="false" private="false" save="true" realtime="false" desc="申请开关"/>
        <property name="Master_Exit_Time" type="int64" public="false" private="false" save="true" realtime="false" desc="离开师门时间 单位级秒"/>
        <property name="Master_Pre_ID" type="string" public="false" private="false" save="true" realtime="false" desc="出师之前的师门"/>
        <property name="Master_Guild_Clone_Num" type="int" public="false" private="false" save="true" realtime="false" desc="师徒刷公会巨魔副本次数限制"/>
        <property name="Real_Logout_Time" type="double" public="false" private="false" save="true" realtime="false" desc="师徒刷公会巨魔副本次数限制"/>
        <property name="Master_Guild_Boss_Time" type="double" public="false" private="false" save="true" realtime="false" desc="师徒刷公会巨魔副本时间"/>
        <property name="Master_Daily_Time" type="double" public="false" private="false" save="true" desc="师门日常领取时间"/> 
        <property name="Master_Power_Award_Num" type="int" public="false" private="false" save="true" desc="师门战力奖励次数"/> 
        <property name="Master_Graduation_Num" type="int" public="false" private="false" save="true" desc="师门出师玩家数量"/>
        <property name="Master_Graduation_Time_Per_Month" type="double" public="false" private="false" save="true" desc="每月出师时间"/>
        <property name="Master_Graduation_Num_Per_Month" type="int" public="false" private="false" save="true" desc="每月出师数量"/>
        <property name="Master_Compensate" type="int" public="false" private="false" save="true" desc="奖励补偿"/>
	</properties>

	<records>
        <record name="master_graduation_record" cols="5" maxrows="10000" save="true" public="false" private="true" desc="出师成员表">
            <column type="string" desc="uid" />
            <column type="widestr" desc="name" />
            <column type="dword"    desc="图标" /> 
            <column type="dword"    desc="战力" />
            <column type="int64"  desc="版本号" vistype="HIDE"/>
        </record>
	</records>
</object>