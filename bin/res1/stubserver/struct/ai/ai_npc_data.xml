<?xml version="1.0" encoding="utf-8" ?>
<object>
    <internals />
    <properties>
        <property name="WarnType" type="int" public="true" private="false" save="false" realtime="" desc="警戒类型(0: 被动  1: 主动球警戒  2: 主动圆柱警戒  3: 主动扇柱警戒  4: 主动球+扇柱警戒)" />
        <property name="StopWarning" type="int" public="false" private="false" save="false" realtime="" desc="是否开启警戒的开关" />
        <property name="SphereRadius" type="float" public="true" private="false" save="false" realtime="" desc="球半径" />
        <property name="CylinderRadius" type="float" public="true" private="false" save="false" realtime="" desc="圆柱半径或扇柱半径" />
        <property name="CylinderLowerHeight" type="float" public="true" private="false" save="false" realtime="" desc="圆柱下高或扇柱下高" />
